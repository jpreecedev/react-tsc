{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true
  },
  "extends": ["standard", "standard-react", "prettier"],
  "rules": {
    "react/prop-types": false
  }
}
